      $("#main-row").prepend(myhtml);
      console.log(myhtml)
    }
  );




}

function select(id)
{
  var elt=$("#l"+id);
  elt.find("img").fadeIn();
  elt[0].style.backgroundColor="#5cb85c";
  elt[0].style.color="white";
  elt[0].getAttribute.class="lyrics-line selected";
  elt.attr("class","lyrics-line selected");
  elt.attr("onClick","deselect("+id+")");
}

function deselect(id)
{
  var elt=$("#l"+id);
  elt[0].style.backgroundColor="white";
  elt[0].style.color="black";
  elt.attr("class","lyrics-line notSelected");
  elt.attr("onClick","select("+id+")");
  elt.find("img").fadeOut(0);

}

function doneSelecting()
{
  $(".lyrics-line.notSelected").fadeOut();
  $(".lyrics-line.selected").css('background-color','white');
  $(".lyrics-line.selected").css('color','black');
  $("#validate-lyrics-button").fadeOut(200);
}


</script>



